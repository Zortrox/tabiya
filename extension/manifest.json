{
  "name": "Tabiya",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Allows complete customization of the new tab page. Default widgets are provided, but you can customize everything 100%.",
  "homepage_url": "https://www.foxslash.com/apps/tabiya",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "override.html"
  },
  "permissions": [
    "unlimitedStorage",
    "storage"
  ]
}